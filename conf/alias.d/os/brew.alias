alias ios='open -a /Applications/Xcode.app/Contents/Developer/Applications/Simulator.app'

alias pkg-add="brew install"
alias pkg-remove="brew uninstall"
alias pkg-update="brew update"
alias pkg-info="brew info"
alias pkg-upgrade="brew upgrade"
alias pkg-outdated="brew outdated --greedy-auto-updates"
alias pkg-lists="brew list"
alias pkg-search="brew search"
alias pkg-clean="brew clean --prune=all"
